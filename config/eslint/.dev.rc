{
  "parser": "babel-eslint",
  "extends": [
    "airbnb-base"
  ],
  "env": {
    "node" : true
  },
  "globals": {
    "__DEV__": false,
    "__PROD__": false,
    "__DEBUG__": false,
    "__root": false
  },
  "rules": {
    "import/no-unresolved": "off",
    "no-console": "off",
    "no-underscore-dangle": "off"
  }
}