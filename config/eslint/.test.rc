{
  "parser": "babel-eslint",
  "extends": [
    "airbnb-base"
  ],
  "env": {
    "node" : true
  },
  "globals": {
    "describe": false,
    "it": false,
    "before": false,
    "after": false,
    "beforeEach": false,
    "afterEach": false
  },
  "rules": {
    "import/no-unresolved": "off",
    "no-console": "off",
    "no-underscore-dangle": "off",
    "no-unused-expressions": "off"
  }
}