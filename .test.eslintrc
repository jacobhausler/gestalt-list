{
  "parser": "babel-eslint",
  "extends": [
    "airbnb-base"
  ],
  "env": {
    "node" : true
  },
  "globals": {
    "Action": false,
    "__DEV__": false,
    "__PROD__": false,
    "__DEBUG__": false,
    "describe": false,
    "it": false,
  },
  "rules": {
    "import/no-unresolved": "off",
    "no-underscore-dangle": "off",
  }
}