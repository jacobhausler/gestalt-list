{
  "name": "node-boilerplate",
  "version": "0.0.1",
  "description": "A minimal Node boilerplate built with ES6 + Webpack",
  "main": "dist/bundle.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "webpack",
    "test": "mocha-webpack --webpack-config webpack.test.config.js \"src/**/test.js\"",
    "dev": "nodemon --watch ./ --exec 'npm run build && npm start'",
    "deploy": "NODE_ENV=production npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.phytel.com/tmartinez/node-boilerplate.git"
  },
  "keywords": [
    "Node",
    "Boilerplate",
    "ES6",
    "Webpack",
    "Babel"
  ],
  "author": "Tyler Martinez",
  "license": "MIT",
  "dependencies": {
    "express": "^4.13.4"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-regenerator": "^6.9.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^2.10.2",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.8.0",
    "mocha": "^2.5.3",
    "mocha-webpack": "^0.4.0",
    "nodemon": "^1.9.2",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "source-map-support": "^0.4.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "yargs": "^4.7.1"
  }
}
